/* For the Grid tables */

table.fs-impact-grid {
	width: 100%;
}

.fs-impact-grid-cell, .fs-impact-grid-header {
	text-align: center !important;
    height: 10px;
    padding: 5px;
}

.fs-impact-grid-cell:first-child, 
.fs-impact-grid-header:first-child{
	display: none;
}

.fs-impact-program-label {
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 10px;
}
.fs-impact-grid-header-row {
    border: 1px solid #D3D3D3;
 	border-right: 0px;
 	border-left: 0px;
    background: url("../images/treas/sub-tab-row-bg.jpg") repeat-x scroll left bottom transparent;
    font-weight: bold;
    padding: 5px 3px;
    text-align: center;
}
.fs-impact-grid-row {
    border-bottom: 1px solid #D3D3D3;
    background-color: #F3F3F3;
    vertical-align: middle;
}

.fs-impact-grid-alternating-row {
    background-color: #e7e7e7;
}

.fs-impact-grid-pager {
	height: 30px;
}
.fs-impact-grid-pager span {
	padding-right: 10px;
}

/* Tool Tips */

a.tt{
    position:relative; /*this is the key*/
    z-index:24;
    border-bottom: 1px #000 dashed;
    color:#000;
    text-decoration:none; cursor:help;}

a.tt:hover{z-index:25; background-color:#f7f7f7; text-decoration: none;}

a.tt span{display: none}

a.tt:hover span{ /*the span will display just on :hover state*/
    display:block;
    padding: 5px; 
    position:absolute;
    top: 2em; 
    left:2em; 
    width:15em;
    border:2px solid #d3d3d3;
    background-color:#f7f7f7; 
    color:#000;
    line-height: 14px;
    font-size: 11px;
    text-align: left;}

