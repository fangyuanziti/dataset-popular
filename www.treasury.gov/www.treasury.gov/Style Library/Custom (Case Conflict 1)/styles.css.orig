/* Layer of Filtering Search Results */
.resultNavigationRegion
{
    padding:8px;
    width: 100%;
    margin: 0 auto;
    white-space:nowrap;
    font-size:1.1em;
}
input.resultPageInput, select.resultPageInput
{
    border-width: 1px; 
	border-style: solid;
	border-color:#DFDFDF;
	border-radius: 3px; 
	-moz-border-radius: 3px; 
	-webkit-border-radius: 3px; 
	-khtml-border-radius: 3px;
