/* begin 		Treasury RR Latest Press Releases 
   Date, Time:	2010-10-19, 0913
*/

.pr-rr-link {display:block; padding:5px 0;}
	.t-rr-pressreleases .rrdate {display:block; color:#5F5F5F; font-style:italic; float:left;}
	/*.pr-rr-link .rrtitle {display:block; width:180px; float:right;}*/
	.t-rr-pressreleases a { text-decoration: none;}
	.t-rr-pressreleases a:hover { text-decoration:underline; }

