/*
	begin		Resource-Center/Data-Chart-Center landing page styling
	Date, Time:	2010-11-22, 1153	
*/
.datawrapper-rc-dcc {padding:20px 0 0 0;width:800px;}
	.inner-wp-rc-dcc {float:left; padding:0 0 0 20px; width:370px;}
		.title-icon a:hover {text-decoration:none;}
	.flash-chart-rc-dcc {float:left;margin-left:-20px;width:370px;}/*margin-left:60px*/
	.datawrapper-rc-dcc H4.ms-standardheader {color:#2a2a2a; font-family:Arial,Helvetica,sans-serif; font-weight:bold; font-size:14px; padding:2px 0 0 0;}
	.datawrapper-rc-dcc h4.ms-standardheader-title {color:#2A2A2A; font-family:Arial,Helvetica,sans-serif; font-size:14px; font-weight:bold; padding:0 0 16px 0;}
	.datawrapper-rc-dcc .ms-WPHeader {background-color:transparent;}
	.datawrapper-rc-dcc  TABLE TD{border:medium none;}
	.datawrapper-rc-dcc .ms-WPHeader TD {border-bottom:medium none;}
	.t-small-rc-dcc, .t-small-rc-dcc p {font-size:12px !important;line-height:12px !important; float:right; width:300px; padding-right:40px;}
.link-disclaimer-container-rc-dcc{width:350px; padding:0 0 0 5px;}
	span.link-rc-dcc{text-align:center; width:350px; display:block; margin-left:-9px; padding:5px 0 0 0;}
	span.disclaimer-rc-dcc{font-size:10px; width:350px; display:block; padding:0 0 0 5px;}
.t-read-more-container{padding:20px 15px 0 0; float:right;}
	.t-read-more-rc-dcc{padding:0 0 5px 0; line-height:18px; margin:0;} 
	.t-read-more-rc-dcc a {background:url("../Images/Treas/right-arrow.gif") no-repeat scroll left 3px transparent; color:#045D9D !important; line-height:18px; padding-left:10px;}
.t-dcc-top-webpart-zone{padding:0 0;}
#other-interest-rate-data{padding:0 0 0 30px; overflow:visible;}
.web-zone-datawrapper-rc-dcc {padding:20px 0 0 0;width :800px;}
.web-zone-flash-chart-rc-dcc {float:left;width:370px; padding:0 0 0 20px;}

