.breaking-news-module h3 {background: url(../images/cpsc-seal.png) no-repeat scroll 1px 3px #E8EFF9;font-family: Arial Narrow, Arial;font-size: 13px;font-weight: bold;padding: 7px 0 6px 30px;margin: 0 0 2px;}
.breaking-news-module .feed-list { border-top: 1px solid #CCCCCC;padding: 0 0 0 11px; border-bottom: 1px solid #ccc; margin: 0 0 2px;}
.breaking-news-module .feed-list ul {margin: 13px 0;height: 200px; overflow-y: auto;}
.breaking-news-module .feed-list li {}
.breaking-news-module .feed-list li .date {font-size: 10px; margin: 0;}
.breaking-news-module .feed-list li p {}
.breaking-news-module .feed-list li p a {}
.breaking-news-module .subscription {padding: 11px 13px;background: #eff4fb;margin: 0 0 8px;}
.breaking-news-module .subscription h4 {font-size: 13px; font-family: Arial Narrow, Arial;margin: 0 0 5px;}
.breaking-news-module .subscription #email_subscription {}
.breaking-news-module .subscription #email_subscription input.text {border: 1px solid #959595;color: #959595;float: left;padding: 5px;width: 120px;}
.breaking-news-module .subscription #email_subscription .orange-btn {float: right;padding:4px; font-size: 13px;}
.breaking-news-module .subscription #email_subscription .orange-btn .corner {}
.breaking-news-module .subscription #email_subscription .orange-btn .corner.tl {left: -6px;top: -4px;}
.breaking-news-module .subscription #email_subscription .orange-btn .corner.tr {right: 6px;top: -4px;}
.breaking-news-module .subscription #email_subscription .orange-btn .corner.bl {left: -6px;bottom: -4px;}
.breaking-news-module .subscription #email_subscription .orange-btn .corner.br {right: 6px;bottom: -4px;}
.breaking-news-module .add-this  {background: url("../images/add-to-site.gif") no-repeat scroll 0 0 transparent;padding: 0 0 0 22px;text-align: center;margin: 0 0 0px 45px}
.breaking-news-module .subscription #email_subscription div {float:left;}

div.ContextMenu div.ContextMenuItem {color: #fff !important;}