div#wsod-business.update { 
	 line-height:16px;
	 padding:6px 5px 0px;
	 border:solid 1px #ccc;
	 border-bottom:0;
	 clear:both;
	 color:#333;
	 position:relative;
	 top:-1px;
	 _top:0px;
	 font-size:11px;
	 _width:323px;
	 }

div#wsod-business div.visibleContent{ 
	 height:1%;
	 }

div#wsod-business div.hiddenContent{ 
	 position: absolute;
	 top:-50000px;
	 }

div#wsod-business table { 
	 width: 216px;
	 padding:0;
	 margin:0;
	 }

div#wsod-business td { 
	 padding:2px;
	 text-align:right;
	 border-bottom:solid 1px #ececec;
	 }

div#wsod-business tr.active td { 
	 background:#f1f4f5;
	 }

div#wsod-business td.first { 
	 text-align:left;
	 }

div#wsod-business td.lastPrice { 
	 font-weight:bold;
	 font-size:11px;
	 }

div#wsod-business td a { 
	 font-weight:bold;
	 text-decoration:none;
	 }

div#wsod-business div.chartContainer { 
	 line-height:12px;
	 padding-top:4px;
	 float:right;
	 text-transform:uppercase;
	 }

div#wsod-business div.chartContainer div.wsodlabel{ 
	 color:#999;
	 }

div#wsod-business div.wsodChart { 
	 overflow:hidden;
	 height:74px;
	 width:100px;
	 border-bottom:solid 1px #ececec;
	 padding-bottom:2px;
	 background-position: top left;
	 background-repeat: no-repeat;
	 }
/* End Markets styles */

#wsodMarkets .querySuggestions .marketQuoteRefer {
         padding: 2px 0 7px 6px;
}
