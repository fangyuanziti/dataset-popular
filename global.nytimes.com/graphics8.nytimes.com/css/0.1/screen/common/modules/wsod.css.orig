/* lower box rules */

#wsodMarkets .box {
	overflow: hidden;
	_float: left;
	_width: 320px;
	margin-top: -1px;
	margin-bottom: 0;	
	padding: 6px;
	border: 1px solid #ccc;
	border-top: 0;
	clear: both;
	}

#wsodMarkets .subColumnA {
	width: 145px;
	margin-right: 0;
	}

#wsodMarkets .subColumnB {
	float: right;
	margin-right: 0;
	width: 163px;
	}

#wsodMarkets .subColumnB .adCreative {
	margin:0 auto;
	}
			
/* quotes form (using marketwatch markup) */

#wsodMarkets .searchForm #qsearchQuery,
#wsodMarkets .searchForm #textIn,
#wsodMarkets .searchForm #hsearchQuery {
	vertical-align: middle;
	width: 112px;
	font-size: 11px;
	line-height: 12px;
	color: #666;
	border: 1px solid #B2B2B2;
	padding: 2px 1px 1px;
	}

#wsodMarkets #wsodFormHome #qsearchQuery,
#wsodMarkets #wsodFormHome #textIn,
#wsodMarkets #wsodFormHome #hsearchQuery {
	width: 134px;
	}

#wsodMarkets .searchForm #searchSubmit {
	vertical-align: middle;
	width: 22px;
	}

#wsodMarkets .searchForm label {
	font-family: arial, helvetica, sans-serif;
	font-size: 1em;
	padding-bottom: 2px;
	text-transform: uppercase;
	display:inline;
	margin-right: 4px;
	}

#wsodMarkets .myPortfoliosBox {
	margin-top:-4px;
	}

#wsodMarkets .myPortfolios {
	font-size: 10px;
	font-weight:bold;	
	font-family: arial, helvetica, sans-serif;
	margin-top:-4px;
	display:block;
	}

/* end quotes form */


/* Begin autosuggest syles for quotes form */
#wsodMarkets .querySuggestions {
        font: normal 12px Arial, Helvetica; 
        position: relative;
        border: 1px solid #ccc;
        background-color: #fff;
        width: 360px !important; /* force scriptaculous to keep width */
        z-index: 100000;
        }


#wsodMarkets .querySuggestions ul {
        list-style-type: none;
        padding: 0;
        margin: 0;
        }

#wsodMarkets .querySuggestions ul li {
        font-size: 12px;
        background: none;
        padding: 1px 5px 1px 80px;
        margin: 0;
        position: relative;
        }

#wsodMarkets .querySuggestions ul li.selected {
        background-color: #eee;
        }

#wsodMarkets .querySuggestions ul li span {
        position: absolute;
        width: 80px;
        top: 1px;
        left: 5px;
}

/* end autosuggest styles for quotes form */



