/* FF 3.6 Zoom Bug Fix */
#home .baseLayout, 
#home .baseLayoutBelowFold {
  width:970px;
}

/* Begin Global Spotlight Carousel */

#home .cColumn div.globalCarouselContainer {
  background: #faf9dc;
  margin: 0 0 8px 0;
  padding-bottom: 6px;
  border-bottom: 1px solid #ccc;
}

#globalCarousel div,
#globalCarousel .story {
  margin-bottom:0 !important;
  display:block;
}

#globalCarousel li.carouselLi {
  font-size:100%;
  background:transparent url(/images/layout/vertical_rule.gif) repeat-y scroll 167px 50%;
  width: 336px;
  text-align: left;
  list-style:none;
  float: left;
  padding:0;
  margin:0 !important;
}

#globalCarousel li.carouselLi li,
#globalCarousel li.carouselLi h5 {
  font-size:1.2em;
}
#globalCarousel li.carouselLi ul.refer li {
  font-size: 1.1em;
}

/* Horizontal Carousel */
#globalCarousel {
  width: 336px;
  position: relative;
  overflow:hidden;
  margin-top: -18px;
  margin-left: 5px;
}

#globalCarousel #globalCarouselSlideContainer {
  border: 1px solid #ccc;
  position: relative;
  overflow: hidden;
  padding:10px 0 10px;
  margin:3px 0 0;
  background:#fff;
  width: 334px;
}

#globalCarousel ul#carouselUl {
  top:0;
  margin: 0;
  padding:0;
  width: 20000px;
}

#globalCarousel .summary {
  margin-bottom:0;
}

/* CAROUSEL NAV */
#globalCarousel #carouselNav {
  background:#faf9dc;
  width:334px;
}

#globalCarousel #carouselNav .sectionHeaderHome {
  margin: 0 0 0 5px;
  float: left;
}

#globalCarousel #carouselNav img {
  border: none;
  float:right;
  margin-top: 4px;
}

#globalCarousel .button {
  width:0;
  height:0;
  line-height:0;
  font-size:0;
  overflow:hidden;
  text-indent:-3000px;
  margin:0 0 -3px;
  padding:0;
  top:0;
  right:10000px;
  border: none;
}

/* Carousel Columns */
#globalCarousel .fullWidth .columnGroup {
  padding-left:6px;
}

#globalCarousel .fullWidth .lastColumn .columnGroup {
  padding-left: 0;
  margin-right: -3px;
}

#globalCarousel .fullWidth .lastColumn  {
  width:173px;
}

#home #globalCarousel .subColumn-2 .column {
  width:169px;
  margin-right:5px;
}

#home #globalCarousel .subColumn-2 .lastColumn {
  margin-right:0 !important;
}

/* End Global Spotlight Carousel */
