	<script type="text/javascript">

	  function load_image(src) {
	  	var ts = new Date().getTime();
		var img = document.createElement("img");
		img.src = src + "&ts=" + ts;
		img.width = 1;
		img.height = 1;
		document.body.appendChild(img);
	  }

	  var pageLoaded = new Date().getTime();
	  var testStart = 0;
	  function gaTracker(params) {
		if(params.length >= 3 && params[2] == "test_loaded") {
			var testLoaded = new Date().getTime();
			_gaq.push(['_trackTiming', 'speedtest', 'testloaded', testLoaded - pageLoaded]);
		}

		if(params.length >= 3 && params[2] == "test_started") {
			testStart = new Date().getTime();
		}

		if(params.length >= 4 && params[2] == "test_complete") {
			if(testStart > 0) {
				var testComplete = new Date().getTime();
				_gaq.push(['_trackTiming', 'speedtest', 'testlength', testComplete - testStart]);
				testStart = 0;
			}

			_gaq.push(['_setCustomVar',2,'test_taken',params[3], 2]); // slot=2, test_taken=count, visit
		}

		// click tracking
		if(params.length >= 3 && params[1] == "advertising") {

			// parse param 3
			var lp_params = params[2].split(";");

			if (lp_params.length == 3) {
				// banner position
				var bp = lp_params[0];	// banner position L = leaderboard, S = skyscraper, F = flash
				var id = lp_params[1];	// impression depth
				var et = lp_params[2];	// vital event type

				var img_src = "/vital/"+ et + ".png?x=" + bp + ";" + id;

				load_image(img_src);

				// reset param 2 so google analytics tracks properly
				if (bp == 'L') {
					params[2] = 'leaderboard_click';
				} else if (bp == 'S') {
					params[2] = 'skyscraper_click';
				} else if (bp == 'F') {
					params[2] = 'endtest_click';
				}
			}
		}

	    _gaq.push(params);
	  }

	  var _gaq = _gaq || [];
	  _gaq.push(['_setAccount', 'UA-389482-3']);
	  _gaq.push(['_setDomainName', 'speedtest.net']);
	  _gaq.push(['_trackPageview']);

