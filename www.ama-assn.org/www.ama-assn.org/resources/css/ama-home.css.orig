/* hover state of nav */

.membership #navigation00, .physician-resources #navigation01, .education-careers #navigation02, .advocacy #navigation03,.medical-journals li#navigation04,.news li#navigation05,.bookstore li#navigation06,.about-ama li#navigation07{ position: relative; color: #e68700;}/* keeps the drop down attached */

#navigation .menu{position:absolute;top:34px;left:0;width:auto;height:auto;z-index:6000;padding:0;
list-style: none;
	position: absolute; 
	left: 0; 
	top: 34px;
	background: #eee;
	margin: 0; padding: 0 0 7px 0;
	display: none;
	float: left;
	border: 1px solid #888;
	border-top: none;
	-moz-border-radius: 0 0 6px 6px;
	-webkit-border-radius: 0 0 6px 6px;
