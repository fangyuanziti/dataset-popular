    });

    $('.fp-tab a').click(function(e){
        var parent = $(this).parent().parent();
        parent.removeClass('not-selected');
        if(parent.hasClass('front-page-stories')){
            $('#breaking-news').hide();
            $('#top-stories').show();
            $('.front-page-breaking-news').addClass('not-selected');
        }else{
            $('#top-stories').hide();
            $('#breaking-news').show();
            $('.front-page-stories').addClass('not-selected');
        }
        e.preventDefault();
    });
});
})(jQuery);
