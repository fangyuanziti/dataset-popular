$( function(){
	var cookie_name = "aaas_postit_2012";
	
  if ( !getCookie(cookie_name) ) {
		// if the cookie doesn't already exist & have a value,
		// then show the post-it and 
		// set the cookie (expires in 14 days)

   document.getElementById('postit_background').style.visibility='visible';
   document.getElementById('postit_box').style.visibility='visible';
	
   $( "#am_postit_00" ).show();
      setCookie(cookie_name, '1', getExpDate(14,0,0));
	}
	
	else {
      // already been seen
      outtamyface();
   }
} );
</script>

