/*
css sprite
-------------------------------------*/
div#header .logo a,div#header ul li a:hover,div#header ul li.selected a,div#header ul li a,
.realico,a.btnSBc,a.btnSBo,.osWindows,.osMac,.osLinux{
background:url(/image/rp002/common/common.png) no-repeat;
}
/* [ for IE6 ] */
a.btnS01,a.btnS01:hover,a.btnSSblue,a.btnSSblue:hover,.realico{
