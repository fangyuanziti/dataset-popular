@import url("base.css");
/*@import url("http://www.sputnikds.com/mymayo.css");*/

body.mymayo {
min-width: 960px; background: #f5f5f3; padding: 0 0 25px 0; 
}

body.mymayo, body.mymayo input, #odin {
font-family: Helvetica, Arial, sans-serif;
}


body.mymayo a, #odin a {
text-decoration: none; color: #007dba;
}

body.mymayo a:hover, #odin a:hover {
text-decoration: underline;	
}

#odin {
z-index: 1000;	
}

#odin #w_head {
background: #f5f5f3; height: 100px; width: 100%; z-index: 100;
}

#odin #logo {
position: absolute; top: 28px; left: 416px; background: url("../img/shieldBottom.png") no-repeat bottom; padding: 0 0 9px 0; z-index: 90; height: 112px;
}

#odin #head {
width: 960px; margin: 0 auto 0 auto;  z-index: 200;
}

#odin #head ul {
margin: 0; padding: 32px 0 0 0;  position: relative; z-index: 100;
}

#odin #head ul li {
list-style: none; margin: 0; padding: 0; width: 110px; height: 35px; position: absolute; font-size: 14px;
}

#odin #head ul li#l_patientcare {
left: 160px; width: 90px; 
}

#odin #head ul li#l_healthinformation {
left: 290px; width: 90px;
}

#odin #head ul li#l_medprofessionals {
left: 580px; width: 95px;
}

#odin #head ul li#l_research {
left: 705px; width: 65px;
}

#odin #head ul li#l_education {
left: 800px; width: 70px;
}

#odin #head ul a {
color: #7e7e74;  font-weight: bold; position: absolute; bottom: 0; text-align: center; padding: 0 0 4px 0;
}

#odin #head ul a:hover {
text-decoration: none;
}

#odin #head ul li:hover, #odin #head ul li.on  {
text-decoration: none; border-bottom: 2px solid #003da5;
}


#odin #head ul li ul {
display: none; background: #f5f5f3 url("../img/arrow_headnav.png") no-repeat 35px top; width: 215px; margin: 0 0 0 0; padding: 18px 22px 12px 22px; top: 35px; border: 1px solid #d5d5d0; border-top: 2px solid #003da5;
}



#odin #head ul li:hover ul {
display: block;
}

#odin #head ul li ul li {
position: relative; float: none; display: block; width: auto; height: auto; font-size: 12px; margin: 0 0 6px 0;
}

#odin #head ul li ul li:hover {
border: 0;
}

#odin #head ul li ul li a {
position: relative; float: none; display: block; text-align: left; font-size: 13px; color: #7e7e74; font-weight: normal;
}

#odin #head ul li ul li a:hover {
text-decoration: underline;
}

#odin #head ul li ul#sub_l_healthinformation {
background-position: 34px top; width: 160px;
}

#odin #head ul li ul#sub_l_medprofessionals {
background-position: 38px top; width: 235px;
}

#odin #head ul li ul#sub_l_research {
background-position: 22px top; width: 160px;
}

#odin #head ul li ul#sub_l_education {
background-position: 24px top; width: 160px; left: 0px;
}

#odin #w_nav {
height: 32px; background: url("../img/bg_nav.png") repeat-x top; z-index: 10;
}

#odin #nav {
width: 960px; margin: 0 auto 0 auto; font-family: Arial, sans-serif;
}


#odin #nav ul {
margin: 0; padding: 0; overflow: hidden;
}

#odin #nav li {
float: left; padding: 0; margin: 8px 0 0 10px; line-height: auto; list-style: none; font-size: 12px;
}

#odin #nav li:first-child {
margin: 8px 0 0 0; padding: 0;
}

#odin #nav a {
color: #7e7f74; font-size: 12px !important;
}

#odin #nav li#l_requestappointment {
margin-left: 90px; border: 0; padding: 0;
}

#odin #nav li#l_findadoctor {
padding: 0; margin-left: 25px;
}

#odin #nav li#l_findajob {
margin-left: 25px; border: 0;
}

#odin #nav li#l_patientlogin {
padding: 0;	margin-left: 150px;
}

#odin #nav li#l_giving {
padding: 0;	margin-left: 25px;
}

#odin #nav li#l_s_facebook {
padding: 0;	margin-left: 25px;
}

#odin #nav li#l_s_facebook a {
text-indent: -999999px; background: url("../img/ico_socmed_nav_fb.png") no-repeat top; width: 16px; height: 16px; display: block;
}

#odin #nav li#l_s_facebook a:hover {
background: url("../img/ico_socmed_nav_fb_on.png") no-repeat top;
}

#odin #nav li#l_s_twitter {
padding: 0;	margin: 9px 0 0 10px;
}

#odin #nav li#l_s_twitter a {
text-indent: -999999px; background: url("../img/ico_socmed_nav_twitter.png") no-repeat top; width: 21px; height: 14px; display: block;
}

#odin #nav li#l_s_twitter a:hover {
background: url("../img/ico_socmed_nav_twitter_on.png") no-repeat top;
}

#odin #nav li#l_s_youtube {
padding: 0;	margin: 10px 0 0 10px;
}

#odin #nav li#l_s_youtube a {
text-indent: -999999px; background: url("../img/ico_socmed_nav_youtube.png") no-repeat top; width: 17px; height: 13px; display: block;
}

#odin #nav li#l_s_youtube a:hover {
background: url("../img/ico_socmed_nav_youtube_on.png") no-repeat top;
}

