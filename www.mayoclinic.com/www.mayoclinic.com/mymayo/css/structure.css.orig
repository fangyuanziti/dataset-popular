#w_banner {
height:398px; background: url("../img/bg_banner.png") repeat-x bottom; z-index: 2; font-size: 13px;
}


#banner {
width: 260px; margin: 0 auto 0 auto; padding: 25px 70px 50px 700px; background: url("../img/head_answers.png") no-repeat 185px 290px;
}

#banner a {
font-size: 20px; font-weight: bold; padding: 0 18px 0 0; margin: 0;
}

#banner p {
color: #444444; margin: 2px 0 25px 0; line-height: 16px;
}

#banner form {
margin: 0 0 25px 0; overflow: hidden;
}

#banner form input[type=text] {
width: 130px; border: 1px solid #9d9d9d; background: #ffffff url("../img/bg_input_text.png") repeat-x left top; padding: 6px; font-size: 14px; color: #454545; float: left; margin: 1px 0 0 0;
}

#banner form input[type="submit"] {
background: #767676 url("../img/bg_btn_blue.png") repeat-x left -32px; color: #ffffff; font-weight: bold; font-size: 13px; padding: 7px 12px 7px 12px; border: 0; margin: 0 0 0 5px; position: relative; cursor: pointer;
border-radius: 2px; -moz-border-radius: 2px; -webkit-border-radius: 2px; 
}

#banner form input[type="submit"]:hover {
background: #767676 url("../img/bg_btn_blue.png") repeat-x left -132px;
}

#banner form input {
width: 80px;
}

#banner form input:first-child {
width: 130px;
}

#banner a.btn {
display: block; background: #1662C6 url("../img/bg_btn_blue.png") repeat-x left -32px; color: #ffffff; font-weight: bold; font-size: 13px; padding:9px 20px 9px 20px; 
border-radius: 2px; -moz-border-radius: 2px; -webkit-border-radius: 2px; text-align: center;
}

#banner a.btn:hover {
background: url("../img/bg_btn_blue.png") repeat-x left -132px; text-decoration: none;
}

#banner .patientstory a {
font-size: 13px; margin: 0; padding: 0;
}

