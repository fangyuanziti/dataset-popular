#w_alerts {
background: #693C5E; padding: 18px 0 18px 0;
}

#alerts {
width: 960px; margin: 0 auto 0 auto;
}

body.mymayo #alerts a {
color: #ffffff; font-size: 16px; font-weight: bold; padding: 0; display: block; width: 100%;
}


#w_sublinks {
background: #ffffff; font-size: 13px;
}

ul#sublinks {
width: 960px; margin: 0 auto 0 auto; padding: 20px 0 20px 0; overflow: hidden;
}

ul#sublinks li {
list-style: none; float: left; width: 280px; margin: 0 0 0 20px; padding: 10px 0 10px 20px; min-height: 80px;
}

ul#sublinks li:first-child {
border: 0; margin: 0; padding: 10px 0 10px 0;
}

ul#sublinks a {
float: left; padding: 0 18px 0 0; font-size: 20px; font-weight: bold; margin: 0 0 8px 0;
}



ul#sublinks p {
clear: left; color: #454545; width: 230px; line-height: 20px;
}


#w_donations {
background-color: #e3e3e0; font-size: 16px;
}

body.mymayo #donations {
width: 960px; margin: auto; color: #444444; padding: 18px 0 18px 0;
}

body.mymayo #donations a {
font-weight: bold; margin: 0 18px 0 0;
}

body.mymayo #donations a.btn {
background: #767676 url("../img/bg_btn_blue.png") repeat-x left -32px; color: #ffffff; font-weight: bold; font-size: 13px; padding: 9px 12px 9px 12px; border: 0; margin: 0 0 0 18px; position: relative; cursor: pointer; border-radius: 2px; -moz-border-radius: 2px; -webkit-border-radius: 2px; line-height: 14px; 
}

body.mymayo #donations a.btn:hover {
background: #767676 url("../img/bg_btn_blue.png") repeat-x left -132px;
}





#w_footer {

}

body.mymayo #footer {
width: 960px; margin: 0 auto 0 auto; color: #919388; font-size: 11px; padding: 25px 0 0 0;
}




body.mymayo #footer div {
float: left; margin: 0 0 0 10px;	width: 150px;
}

body.mymayo #footer div:first-child {
margin: 0;
}

body.mymayo #footer a {
color: #919388;
}

body.mymayo #footer ul {
margin: 0 0 25px 0; padding: 0;
}

body.mymayo #footer ul li {
margin: 0 0 6px 0; list-style: none; padding: 0;
}

body.mymayo #footer ul li#sm_facebook, #footer ul li#sm_youtube, #footer ul li#sm_twitter {
background: none; padding: 0; margin: 0 0 8px 0;
}

body.mymayo #footer ul li#sm_facebook {
margin-top: 10px;
}

body.mymayo #footer ul li#sm_facebook a {
background: url("../img/icon_socialmedia.png") no-repeat left 0; padding: 1px 0 4px 22px;
}

body.mymayo #footer ul li#sm_youtube a {
background: url("../img/icon_socialmedia.png") no-repeat left -23px; padding: 1px 0 4px 22px;
}

body.mymayo #footer ul li#sm_twitter a {
background: url("../img/icon_socialmedia.png") no-repeat left -47px; padding: 1px 0 4px 22px;
}

body.mymayo #footer h4 {
font-weight: bold; font-size: 11px; margin: 0 0 12px 0; color: #919388;
}


body.mymayo #footer #copyright {
clear: both; border-top: 1px solid #d3d3d3; padding: 12px 0 0 0; margin: 25px 0 0 0; float: none; width: 100%;
}

body.mymayo #footer #copyright p {
text-align: right;
}

body.mymayo #footer #copyright ul {
margin: 0; padding: 0;
}

body.mymayo #footer #copyright ul li {
float: left; margin: 0 0 0 10px; padding: 0 0 0 10px; border-left: 1px solid #d3d3d3; list-style: none; background: none;
}

body.mymayo #footer #copyright ul li:first-child {
border: 0; margin: 0; padding: 0;
}