function slider_cb() {
  var coverImage = "http://media.economist.com/sites/default/files/slider/currentcoveruk.jpg";
  var currentTime = new Date();
  document.write('<img src="' + coverImage + "?" + (currentTime.getMonth()+1) +"-"+ currentTime.getDate() + '" width="120" height="155" border="0"/>'); 
}

