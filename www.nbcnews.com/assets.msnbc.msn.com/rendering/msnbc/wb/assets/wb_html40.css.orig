/* New designed story toolbar */
#stryTools {
background: #FFF url(http://media1.s-nbcnews.com/i/msnbc/Components/CSS/img/bg-stryTools.png) repeat-x 0 0;
clear: both;
height: 75px;
margin-top: 20px;
font-family: Arial, Helvetica, Sans-Serif;
line-height: normal;
}

#stryTools ul,#stryTools ul li { 
margin: 0;
padding: 0;
list-style: none;
}

#stryTools ul {
margin-left: 30px;
}
#stryTools ul li {
float: left;
width: 33%;
font-size: 11px;
height: 30px;
}

#stryTools ul .discuss {
width: 34%;
}

#stryTools ul .discuss a {
padding-top: 10px;
background-position: 0 11px;
}

#stryTools ul .rate {
width: 51%;
}

#stryTools ul .email {
clear: left;
width: 29%;
}
#stryTools ul .im {
width: 41%;
}
#stryTools ul .print {
clear: right;
width: 20%;
}

#stryTools ul em {
font-size: 10px;
font-style: normal;
}

#stryTools ul .rate em.avg {
display: block;
margin-left: 9em;
color: #666;
}

#stryTools ul .discuss em {
display: block;
margin: -5px 0 0 20px;
}
#stryTools ul .rate {
padding: 8px 0 6px 0;
}

#stryTools ul .rate em {
margin: 0 6px;
color: #999;
}

#stryTools ul .rate b {
float: left;
width: 6.5em;
margin-top: 2px;
text-align: right;
margin-right: 1ex;
}

#stryTools ul .rate b a {
font-size: 10px;
font-weight: normal; 
padding: 0;
}

#stryTools ul a {
display: block;
padding: 10px 0 6px 14px;
background-position: 0 10px;
background-repeat: no-repeat;
font-weight: bold;
}

#stryTools ul .rate b a:link,#stryTools ul .rate b a:visited {
color: #369;
}

#stryTools ul .rate b,#stryTools ul a:link,#stryTools ul a:visited {
color: #000;
}

#stryTools ul a:hover {
color: #C00;
}

#stryTools ul .print a {
background-image: url(http://media1.s-nbcnews.com/i/msnbc/Components/ArtAndPhoto-Fronts/SITEWIDE/Icons/icon-print.png);
line-height: 10px;
width: 4em;
}

#stryTools ul .im a {
background-image: url(http://media1.s-nbcnews.com/i/msnbc/Components/ArtAndPhoto-Fronts/SITEWIDE/Icons/icon-im.png);
line-height: 10px;
width: 10em;
}

#stryTools ul .email a {
background-image: url(http://media1.s-nbcnews.com/i/msnbc/Components/ArtAndPhoto-Fronts/SITEWIDE/Icons/icon-email.png);
line-height: 10px;
width: 4em;
}
#stryTools ul .discuss a {
background-image: url(http://media1.s-nbcnews.com/i/msnbc/Components/ArtAndPhoto-Fronts/SITEWIDE/Icons/icon-discuss.png);
}

/* off element */
#stryTools.no-toolbar,
#stryTools.no-email li.email,
#stryTools.no-im li.im,
#stryTools.no-print li.print,
#stryTools.no-discuss li.discuss,
#stryTools.todayshow li.rate,
#stryTools.no-rate li.rate {
display: none;
}

#stryTools.todayshow ul li.discuss,
#stryTools.no-rate ul li.discuss {
width: 95% !important;
padding-top: 16px;
height: 26px;
line-height: 22px;
}

#stryTools.todayshow ul li.discuss a,
#stryTools.todayshow ul li.discuss em,
#stryTools.no-rate ul li.discuss a,
#stryTools.no-rate ul li.discuss em {
display: inline;
}
#stryTools.todayshow ul li.discuss em,
#stryTools.no-rate ul li.discuss em {
margin: 0;
}

#stryTools.todayshow {
background-position: 0 -1px;
}

/* with sponsor */
div.sponsor {
position: relative;
}

#stryTools.sponsor p.ad {
position: absolute;
top: 1px;
right: 0;
width: 125px;
height:40px;
