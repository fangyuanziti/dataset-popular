#gnotbar{
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	margin:5px auto;
	border:1px solid #999;
	background: #eee; /* fallback */
	/*background-image: -moz-linear-gradient(100% 100% 90deg, #eee, #dedede);
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#eee), to(#dedede));*/
	background-color:#ffffba;
	padding:12px 10px;
	color:#000;
	font-size:1.2em;
	position:relative;
	width:966px;
}
#gnotbar.dnone{
	display:none;
}
#gnotbar a{
	text-decoration:none;
	color:#3e88ae;
}
#gnotbar a:hover{
	text-decoration:underline;
}
#gnotbarclose, #gnotbarclose a{
	height:0;
	padding-top:20px;
	overflow:hidden;
	width:20px;
}
#gnotbarclose{
	position:absolute;
	right:10px;
	top:11px;
	z-index:10;
}
#gnotbarclose a{
	background: url('../gfx/sprites.gif') no-repeat -99px -317px;
}

.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden; }

/* IE7 Fix */
.clearfix { zoom: 1; }
/* Hides from IE-mac \*/
* html .clearfix {height: 1%;
}
/* End hide from IE-mac */

