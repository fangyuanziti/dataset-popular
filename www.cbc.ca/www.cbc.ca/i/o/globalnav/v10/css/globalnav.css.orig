	border: 0;
	vertical-align: -3px;
}
#gn.v10 #gnav {
	color: #000;
	font-family: verdana;
	background-color:#1a1a1a;
	height:auto;
}
#gn.v10 #gnav ul {
	margin:0;
	padding: 6px 0 0;
	list-style-type: none;
	height: 37px;
	overflow: hidden;
}


