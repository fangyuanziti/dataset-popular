	margin: 0 1px;
	padding: 0;
	list-style-type: none;
	display: inline;
	float: left;
	overflow: hidden;
	height: 37px;
	position:relative;
}
#gn.v10 #gnav li.btn a {
	color: #fff;
	display: block;
	padding: 8px 3px 10px 5px;
	position: relative;
	text-decoration: none;
	overflow: hidden;
	height: 13px;
	float: left;
	margin-bottom: 6px;	
}
