img {
	border: 0;
}
body {
	margin: 0;
	font:12px Arial,sans-serif;
}
#accessibilitylinks{
	position:relative;
}
#accessibilitylinks h2{
	height: 0;
	margin: 0;
	padding:0;
	overflow: hidden;
	float:left;
	line-height:0;
}
#accessibilitylinks ul{
	list-style-type:none;
	padding:0;
	margin:0;
	height:0;
	overflow:hidden;
	float:left;
}
#accessibilitylinks ul li a{
	color:#000;
	text-decoration:none;
	font-weight:bold;
	background-color:#fffe9d;
	position:absolute;
	left:-200%;
}
#accessibilitylinks ul li a:focus, #accessibilitylinks ul li a:active{
	position:absolute;
	width:974px;
	top:0;
	left:0;
	z-index:100;
	padding:5px;
	border:1px solid #c8c8c8;
	outline:0;
}
#cbccontentholder{
	position:absolute;
	left:-200%;
}

#gnwrap {
	background-color:#1a1a1a;
	width:986px;
	height:138px;
}
#gnwrap.open, #gnwrap.core {
	height:auto;
}

#gnadww {
	width:964px;
	padding:5px 8px 0 14px;
}

#gn.v10 {
	overflow: hidden;
	font-family: arial, sans-serif;
	font-size: 0.92em;
	position: relative;
	padding:0 5px 5px;
