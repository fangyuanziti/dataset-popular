	background: url('../gfx/sprites.gif') no-repeat -3px -87px;
	/*background-color:#fff;
	color:#000;
	padding: 8px 3px 10px 5px;
	padding: 6px 3px 6px 5px;*/

}
#gn.v10 #gnav ul li.btn.active a.caret, #gn.v10 #gnav ul li.emboss.btn.active a.caret { /*white bg*/
	background: url('../gfx/sprites.gif') no-repeat -197px -44px;
}
