/*footer*/
#footer.v10{
	background-color:#1e1e1e;
	color:#fff;
	width:986px;
	font-family:arial,sans-serif;
	font-size:1em;
	padding:0;
	margin:0;
}
#footer.v10 h2{
	height:0;
	margin:0;
	overflow:hidden;
}
#footer.v10 ul{
	list-style-type:none;
	float:left;
	padding:0;
	margin:0;
}
#footer.v10 ul li{
	margin:0;
	line-height:1.17em;
}
#footer.v10 ul li a{
	font-size:0.92em;
	color:#b1b1b1;
	text-decoration:none;
}
#footer.v10 ul li a:hover{
	text-decoration:underline;
}

#footer.v10 #ftr-ptls{
	padding:20px 13px 17px;
}

#footer.v10 .main {
	float:left;
	width:480px;
}
#footer.v10 .main ul{
	width:160px;
}
#footer.v10 .main.corp ul{
	width:240px;
}
#footer.v10 .main ul li {
	padding:3px 10px;
}
#footer.v10 .main h3{
	font-size:1.67em;
	margin:0 0 12px 9px;
}
#footer.v10 .main.corp h3{
	text-transform:uppercase;
	font-size:1.08em;
	margin:7px 0 14px 10px;
}
#footer.v10 #ftr-connect h3{
	float:left;
	margin:0;
	margin-right:25px;
	font-size:1.08em;
}
#ftr-connect{
	padding:12px 23px;
	text-transform:uppercase;
	border-top:1px solid #5c5c5c;
}
#ftr-connect ul{
	width:780px;
	margin-top:5px;
} 
#ftr-connect ul li{
	line-height:0.67em;
	float:left;
	padding:0 15px;
	border-right:1px solid #5c5c5c;
}
#ftr-connect ul li.last {
	border-right:none;
}
#ftr-connect ul li a{
	color:#fff;
	font-size:0.75em;
}

#ftr-connect ul li span{
	margin-right:10px;
	color:#bbb;
	font-size:0.75em;
}

#ftr-core{
	position:relative;
	padding:20px 8px;
	background-color:#000;
	border-top:1px solid #5c5c5c;
}
