/*weather*/
#gnweather{
	width:200px;
	height:77px;
	border:1px solid #000;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	color:#fff;
	font-size:1em;
	
	background-color: #333; /* fallback */
	background-image: -moz-linear-gradient(100% 100% 90deg, #1e1e1e, #333);
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, to(#1e1e1e), from(#333));

	-webkit-box-shadow: 0 1px 0 #444;
	-moz-box-shadow: 0 1px 0 #444;
	/*box-shadow: 0 1px 0 #444;*/

	float:left;
	display:inline;
	margin:0;
	padding:5px 10px 5px 0;
	overflow:hidden;
}
/*end weather*/
