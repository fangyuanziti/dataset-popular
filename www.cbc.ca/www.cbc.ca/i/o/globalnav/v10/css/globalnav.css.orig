	margin:0;
	overflow:hidden;
}

#ftr-core ul.terms{
	float:none;
	/*width:100%;*/
}
#ftr-core ul.terms li a{
	color:#fff;
	letter-spacing:0.5px;
}
#ftr-core ul li{
	float:left;
	padding:0 15px;
}

#ftr-cbcrc{
	background:url("../gfx/sprites.gif") no-repeat -2px -930px;
	color:#fff;
	height:0;
	width:153px;
	padding-top:21px;
	overflow:hidden;
	position:absolute;
	top:15px;
	right:23px;
}

#ftr-end {
	font-size:0.92em;
	padding:6px 0 0 14px;
}
#ftr-end span {
	color:#b1b1b1;
	float:left;
}
#ftr-end ul.affs {
	float:right;
}
#ftr-end ul.affs li a{
	font-size:1em;
}
