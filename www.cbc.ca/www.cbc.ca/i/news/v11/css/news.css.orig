/* Must Watch module */
.mustWatch.alt { background: #eee; }

.mustWatch { padding-bottom: 0; margin-bottom: 20px; }
.mustWatch h3 { margin: 5px; }
.mustWatch li {
	position: relative; display: inline; width: 140px; float: left; margin: 0 5px 10px;
	height: 187px; overflow: hidden;
	}
.mustWatch li a:hover { text-decoration: none; }
.mustWatch img { z-index: 1; }
.mustWatch .playbtn {
	position: absolute; top: 48px; left: 0; z-index: 10;
	background: url(../gfx/play.png) top left no-repeat;
	zoom: 1; width: 32px; height: 32px;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=60)";
	filter: alpha(opacity=60);
	opacity: .6;
	}
.mustWatch a:hover .playbtn {
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	filter: alpha(opacity=100);
	opacity: 1;
	}
.mustWatch li h4 { font-size: 1.1em; font-weight: bold; padding: 5px 0; }
.mustWatch li a:hover h4 { text-decoration: underline; }
.mustWatch li p { color: #000; padding: 0 0 5px; }
.mustWatch li p.length { font-size: .9em; color: #666; }

.mustWatchClips { min-height: 350px; }


