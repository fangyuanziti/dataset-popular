/* news button styles */
a.newsbutton {
	margin: 0;
	padding: 5px 10px;
	text-align: center;
	font-size: 11px;
	border: none;
	color: #FFF;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	background: #125b86; /* Old browsers */
	background: -moz-linear-gradient(top,  #125b86 0%, #094162 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#125b86), color-stop(100%,#094162)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #125b86 0%,#094162 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #125b86 0%,#094162 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #125b86 0%,#094162 100%); /* IE10+ */
	background: linear-gradient(top,  #125b86 0%,#094162 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#125b86', endColorstr='#094162',GradientType=0 ); /* IE6-9 */
}
a.newsbutton:hover,
a.newsbutton:focus {
	cursor: pointer;
	text-decoration: none;
	background: #666666; /* Old browsers */
	background: -moz-linear-gradient(top,  #666666 0%, #666666 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#666666), color-stop(100%,#666666)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #666666 0%,#666666 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #666666 0%,#666666 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #666666 0%,#666666 100%); /* IE10+ */
	background: linear-gradient(top,  #666666 0%,#666666 100%); /* W3C */
