


/* START - Pop Ups */





#modalOverlay {
background-color: #000;
cursor: wait;
}

#modalContainer {
width: 600px;
left: 50%;
top: 15%;
margin-left: -300px;
}

.free_popup {
width: 315px;
background-color: transparent;
position:absolute;
z-index: 10;
background-image: url(../../media/new-oct09/img-popup-direction-arrow-pixy.png);
background-repeat: no-repeat;
display:inline
}

.popup_left {
margin-top:-17px;
margin-left: 3px;
padding: 0 0 0 24px;
background-position: left top;
float:right;
}

.popup_right {
padding: 0 28px 0 0;
background-position: right top;
}


.popup_container {
font-family: Arial, Helvetica, sans-serif;
width: 296px;
padding: 2px 4px 4px 12px;
margin: 0;
-moz-box-shadow:8px 0px 14px #888;
-webkit-box-shadow:8px 0px 14px #888;
-box-shadow:8px 0px 14px #888;
/*border: #f7ab04 solid 4px;*/
border: #b20000 solid 4px;
background-color: transparent;
}

.border_left_off {
border-left: none;
}

.border_right_off {
border-right: none;
}

