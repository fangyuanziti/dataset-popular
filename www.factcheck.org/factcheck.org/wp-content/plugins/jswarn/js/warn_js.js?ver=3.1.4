jQuery.noConflict();
jQuery(document).ready(function(){
	jQuery('#publish').click(function(){
		var ddConfirmPublish = confirm('Are you sure you wish to publish the current article?\n\n Press \'OK\' to publish. Press \'cancel\' to go back to the article and continue editing.');
		if(ddConfirmPublish){
			return true;
		}
		else{
			alert('Post was not published. Whew. That was close!');
			return false;
		}
	});
});
