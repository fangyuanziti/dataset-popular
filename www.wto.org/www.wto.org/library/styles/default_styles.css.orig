.megamenu{
position: absolute;
display: none;
left: 0;
top: 0;
background-color: #F0F0F0;
border: 1px solid #CCCCCC;
border-width: 1px;
padding: 0 5px 5px 10px;
font-size: 8pt; 
font-family: Trebuchet MS, Verdana, Arial, Helvetica, sans serif; 
color: #666666;
z-index: 100;
font-style: normal; 
font-weight: normal;

}

.megamenu .column{
/*float: left;*/
width: 170px; /*width of each menu column*/
/*margin-right: 5px;*/
vertical-align:top;
padding:0 10px 5px 0px;
}

.megamenu .column ul{
margin: 0;
padding: 0;
list-style-type: none;
}

.megamenu .column ul li{
padding-bottom: 3px;
}

.megamenu .column h3{
color: #3399FF;
font-size: 10pt;
font-weight: bold;
font-family: Trebuchet MS, Verdana, Arial, Helvetica, sans serif; 
padding-top: 3px;
margin: 0 0 5px 0;
}
.megamenu .column h3 a{
color: #3399FF;
font-size: 10pt;
font-weight: bold;
font-family: Trebuchet MS, Verdana, Arial, Helvetica, sans serif; 
padding-top: 3px;
margin: 0 0 5px 0;
text-decoration: none;
}

.megamenu .column ul li a{
text-decoration: none;
}

.megamenu .column a:hover{
	color: #CC6600;
	text-decoration: underline;
}
