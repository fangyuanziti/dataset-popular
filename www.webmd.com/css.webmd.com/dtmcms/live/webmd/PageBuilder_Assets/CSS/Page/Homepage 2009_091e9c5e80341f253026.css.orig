****************************/

/* IE6 hack to set height of Stay Informed module */
.header_w_button_rdr#stay-infrmd {
_height: 75px;
}

/* START Stay informed newletter module */
.header_w_button_rdr#stay-infrmd {
margin-bottom: 25px;
width: 482px; 
min-height: 75px;
}

.header_w_button_rdr#stay-infrmd h4 {
border-bottom: 1px solid #DFE5D5;
color: #88A906;
font-size: 180%;
font-weight: normal;
margin: 0;
padding: 0 0 8px 0; 
}
.header_w_button_rdr#stay-infrmd ul {
margin: 10px 0px 0px 5px;
}
.header_w_button_rdr#stay-infrmd ul li {
font-size: 90%;
line-height: 1.4em;
margin: 0;
padding: 0;
}
.header_w_button_rdr#stay-infrmd .button_img {
margin: 0;
border: none;
padding-left: 6px;
position: relative;
top: -3px;
vertical-align: text-top;
}
.header_w_button_rdr#stay-infrmd a:link, .header_w_button_rdr#stay-infrmd a:visited, .header_w_button_rdr#stay-infrmd a:hover, .header_w_button_rdr#stay-infrmd a:active {
text-decoration: none;
}
/* END Stay informed newletter module */

