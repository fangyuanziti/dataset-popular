      ldcc_string += 'l_' + (ldcc_response["Profile"]["Audiences"]["Audience"][k]["abbr"]) + 'x';
    });

    webmd.cookie.setJson('ads_perm', {"segvar": ldcc_string}, {domain: "webmd.com"});
  };

  webmd.load({js: '//ad.crwdcntrl.net/5/c=932/pe=y/callback=processauds'});

})();

        
