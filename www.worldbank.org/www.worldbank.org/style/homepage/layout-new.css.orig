}

.type, .date, .author {color: #666; margin: 2px 0px; font-weight: normal;}

.date {font-size: 0.916em;}

p {margin: 10px 0px;}

/* INLINE ELEMENTS */

strong {font-weight: bold;}

em, 
cite {font-style: italic;}

acronym,
abbr { font-style: normal;
