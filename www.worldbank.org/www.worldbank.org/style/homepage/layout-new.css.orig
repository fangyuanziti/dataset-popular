@charset "utf-8";
/* CSS Document */

@import "ff-fix.css";


body {
background: #dadccf;
margin:0px 0px 20px; padding:0px; 
text-align:center; /*workaround for centering div in IE*/ 
}

a, a:visited, a:hover{text-decoration: none; color: #850000;}
a:hover {text-decoration: underline;color: #850000; }


h1 a, h1 a:visited, 
h2 a, h2 a:visited, 
h3 a, h3 a:visited, 
h4 a, h4 a:visited, 
