#nihdirector a.more span {
 	position: absolute;
 	left: -9000px;
 	width: 1px;
 	height: 1px;
 	overflow: hidden;
}
#nihdirector a.blog {
	display: block;
	min-height: 19px;
	padding-top: 1px;
	padding-left: 23px;
	background: transparent url(../images/icon-color-blog.gif) no-repeat left top;
}
#nihdirector a.twitter {
	display: block;
	min-height: 19px;
	padding-top: 1px;
	padding-left: 23px;
	background: transparent url(../images/icon-color-twitter.gif) no-repeat left top;
}
#nihdirector a.email {
	display: block;
	min-height: 19px;
	padding-top: 1px;
	padding-left: 23px;
	background: transparent url(../images/icon-color-email.gif) no-repeat left top;
}
#nihdirector a.rss {
	display: block;
	min-height: 19px;
	padding-top: 1px;
	padding-left: 23px;
	background: transparent url(../images/icon-color-rss.gif) no-repeat left top;
}
#nihdirector a.leadership {
	display: block;
	height: 18px;
	padding-top: 2px;
	padding-left: 23px;
	background: transparent url(../images/icon-color-leadership.gif) no-repeat left top;
}
#nihdirector a.more {
	position: relative;
	left: -1px;
	display: block;
	width: 56px;
	height: 24px;
	margin-top: 4px;
	background: transparent url(../images/button-more-brown.gif) no-repeat left top;
	-webkit-box-shadow: 1px 1px 5px rgba(0,0,0,0.2);
	box-shadow: 1px 1px 5px rgba(0,0,0,0.2);
}
#nihdirector form {
	margin-top: 4px;
}
#nihdirector legend {
	color: #444;
	padding: 0 0 4px 0;
	margin: 0;
}
#nihdirector #email {
	font: normal 0.9em/1.4em Trebuchet MS, Arial, Helvetica, sans-serif;
	color: #757575;
	width: 150px;
	height: 18px;
	padding-left: 4px;
	margin-top: 1px;
	margin-right: 4px;
	border: 1px solid #bfbfbe;
}
#nihdirector .form_button {
	margin-top: 4px;
	-webkit-box-shadow: 1px 1px 5px rgba(0,0,0,0.2);
	box-shadow: 1px 1px 5px rgba(0,0,0,0.2);
}


